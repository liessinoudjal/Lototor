{% extends 'base.html.twig' %}

{% block title %}Publier un loto{% endblock %}

{% block body %}
<section  class="container jumbotron">
  <ul class="nav nav-tabs">
    <li class="nav-item">
      <a class="nav-link active" data-toggle="tab" href="#live">Loto live</a>
    </li>
    <li class="nav-item">
      <a class="nav-link d-none" title="Pas disponible pour le moment" data-toggle="tab" href="#indoor">Loto en salle</a>
    </li>

  </ul>
  <div id="myTabContent" class="tab-content">
    <div class="tab-pane fade active show" id="live">
      {% include "account/form/_live_loto_form.html.twig" with {"form" :liveLotoForm, "button_label": "Publier"}%}
    </div>
    <div class="tab-pane fade" id="indoor">
      {% include "account/form/_indoor_loto_form.html.twig" with {"form" :indoorLotoForm, "button_label": "Publier"}%}
    </div>
  </div>
</section>

{% endblock %}